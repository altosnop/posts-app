{{#each this}}
  <div class='col-sm-4 mb-3'>
    <div class='card'>
      <div class='card-body'>
        <h5 class='card-title'>{{title}}</h5>
        <p class='card-text'>{{body}}</p>
        <button class='btn btn-primary'>Edit</button>
        <button class='btn btn-danger'>Delete</button>
      </div>
    </div>
  </div>
{{/each}}